{
  "private": true,
  "name": "mx-docs",
  "scripts": {
    "start": "dumi dev",
    "dev": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "dumi": "1.1.39",
    "lint-staged": "12.1.7",
    "prettier": "2.5.1",
    "yorkie": "2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mx-space/docs.git",
    "branch": "master",
    "platform": "github"
  }
}